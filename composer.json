{
    "name": "mgomezbuceta/cakephp-aclmanager",
    "description": "Modern Authorization Manager for CakePHP 5.x - Role-based permission management system",
    "keywords": ["cakephp", "cakephp5", "cake5", "plugin", "authorization", "manager", "permissions", "rbac", "roles"],
    "homepage": "https://github.com/mgomezbuceta/cakephp-aclmanager",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos GÃ³mez Buceta",
            "email": "mgomezbuceta@gmail.com",
            "homepage": "https://github.com/mgomezbuceta",
            "role": "Maintainer"
        },
        {
            "name": "Ivan Amat",
            "homepage": "http://www.ivanamat.es",
            "role": "Original Author"
        }
    ],
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0",
        "cakephp/authorization": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "cakephp/cakephp-codesniffer": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "AclManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AclManager\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "cakephp/plugin-installer": true
        }
    }
}
